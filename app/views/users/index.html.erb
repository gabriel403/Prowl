<div class="page-header">
  <h1>Users</h1>
</div>
<table class="table">
  <thead>
    <tr>
      <th>Email</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= link_to user.email, user %></td>
        <td>
          <%= link_to 'Edit', edit_user_path(user) %>
          <%= link_to 'Delete', user_path(user), :method => 'delete', :confirm => 'Are you sure?' %>
        </td>
      </tr>
    <% end %>
    <tr><td colspan="2"><%= link_to 'New User', new_user_path %></td></tr>
  </tbody>
</table>
